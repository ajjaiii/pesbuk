{
  "containerDefinitions": [
    {
      "name": "pesbuk",
      "image": "081602682094.dkr.ecr.us-west-2.amazonaws.com/ahmad-repo:BUILD_NUMBER",
      "cpu": 0,
      "memory": 128,
      "portMappings": [
        {
          "containerPort": 80
        }
      ],
      "dockerLabels" : {
        "container" : "pesbuk",
        "environment" : "development"
      },
      "logConfiguration" : {
        "logDriver": "json-file",
        "options": {
          "labels" : "container,environment"
        }
      }
    }]      
}
